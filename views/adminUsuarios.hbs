<div class="container-fluid  bg-dark" style="height : 100vh">
    <div class="container">


        <table class="table text-white">
            <tr>
                <th>#</th>
                <th>Nombre</th>
                <th>Apellido</th>
                <th>Telefono</th>
                <th>Direccion</th>
                <th>Imagen</th>
                <th>-</th>
                <th>-</th>
            </tr>
            {{#each usuarios}}
            <tr>
                <td>{{id}}</td>
                <td>{{username}}</td>
                <td>{{pass}}</td>
                <td>{{mail}}</td>
                <td>{{telefono}}</td>
                <td>{{habilitado}}</td>
                <td>{{eliminado}}</td>


                <td><a href="/admin/empleados/update/{{id}}" class="btn btn-primary"><i class="fas fa-edit"></i>
                        Editar</a></td>
                <td><a href="/admin/empleados/delete/{{id}}" class="btn btn-danger"><i class="fas fa-trash-alt"></i>
                        Eliminar</a></td>
            </tr>
            {{/each}}
        </table>

    </div>
</div>